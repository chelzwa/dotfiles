# Bunch of symlinks
ln -sfv "$HOME/config/.zshrc" ~
ln -sfv "$HOME/config/.gitignore_global" ~
ln -sfv "$HOME/config/.gitconfig" ~
ln -sfv "$HOME/config/Preferences.sublime-settings" $HOME/Library/Application\ Support/Sublime\ Text\ 3/Packages/User
ln -sfv "$HOME/config/sublime_snippets" $HOME/Library/Application\ Support/Sublime\ Text\ 3/Packages/User
mkdir -p /etc/resolver
ln -sfv "$HOME/config/nginx/dev.resolver" /etc/resolver/dev
ln -sfv "$HOME/config/nginx/dev.nginx" /usr/local/etc/nginx/sites-enabled/dev
ln -sfv "$HOME/config/nginx/dev.dnsmasq" /usr/local/etc/dnsmasq.conf
